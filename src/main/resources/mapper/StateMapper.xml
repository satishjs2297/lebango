<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lebango.mapper.StateMapper" >   
    <select id="getAll" resultType="com.lebango.bean.State">
		SELECT 
			* 
		FROM 
			states
	</select>
	
	<select id="getById" resultType="com.lebango.bean.State">
		SELECT 
			* 
		FROM 
			states
		WHERE
			state_id = #{state_id}
	</select>
</mapper>